<%= simple_form_for @food_tour do |f| %>
  <%= f.input :title %>
  <%= f.input :description %>
  <%= f.input :city %>
  <%= f.input :price %>
  <%= f.input :dates %>
  <%= f.input :cuisine %>
  <%= f.input :photo %>
  <%= f.input :photo_cache, as: :hidden %>
  <%= f.button :submit, class: 'btn btn-primary' %>
<% end %>
