<%= simple_form_for [@food_tour, @booking] do |f| %>
    <%= f.input :date, as: :string, label: false, required: false, input_html: {class: "datepicker form-inline"} %>
    <%= f.button :submit, id: 'sweet-alert-demo', class: 'btn btn-primary' %>
<% end %>

<input type="hidden" value="<%= @food_tour.start_date %>" id="foodTourStartDate">
<input type="hidden" value="<%= @food_tour.end_date %>" id="foodTourEndDate">
